
{
	"description": "Cache requests to GoogleMaps for geoguessr",
	"manifest_version": 2,
	"name": "GeoCache",
	"version": "1.0",

	"icons": {
		"48": "icons/icon-48.png"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],

	"background": {
		"scripts": ["cacher.js"]
	}
}
