{
	"name": "GeoCache",
	"description": "Cache requests to GoogleMaps for geoguessr",
	"manifest_version": 3,
	"version": "1.0",
	"icons": {
		"48": "icons/icon-48.png"
	},
	"permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestFeedback"
	],
	"host_permissions": [
		"*://*.google.com/*",
		"*://streetviewpixels-pa.googleapis.com/*",
		"*://*.geoguessr.com/*"
	],
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "1",
				"enabled": true,
				"path": "rule.json"
			}
		]
	}
}